        "test": "npx vitest",
        "pre-commit": "npx lint-staged",
        "coverage": "npx vitest run --coverage"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest",
            "plugin:tailwind/recommended"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
