{
    "name": "@use-miller/frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "vite",
        "build:netlify": "cd ../../libs/shared-api-client && yarn --check-files && cd ../../apps/frontend && yarn install --check-files && vite build",
