{
    "name": "@use-miller/app-backend-api",
    "version": "1.0.1",
    "description": "",
    "author": "Use Miller",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "up": "mkdir -p ./.docker-compose/volumes/postgres ./.docker-compose/volumes/redis && chmod -R 777 ./.docker-compose && docker compose up -d",
        "down": "docker compose down",
        "build": "pnpm rimraf dist && pnpm nest build --tsc --path tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/src/main",
        "lint": "npx eslint -c .eslintrc.js --ext .ts src/**/*.ts --fix",
        "test": "jest --config=jest.config.ts --runInBand --color --reporters=default --no-cache --coverage=false --passWithNoTests",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ../../node_modules/@darraghor/nest-backend-libs/dist/database-postgres/PostrgresMigrationsHelper.js",
        "gen": "npm run typeorm migration:generate ",
        "db": "typeorm-ts-node-commonjs -d node_modules/@darraghor/nest-backend-libs/dist/database-postgres/PostrgresMigrationsHelper.js",
        "db:gen": "npm run build && npm run db migration:generate",
        "db:run": "npm run db migration:run",
        "db:rev": "npm run db migration:revert",
        "stripe:wh": "stripe listen --forward-to http://localhost:34522/payments/stripe/webhook-receiver",
        "client:generate": "pnpm miller api:write-client --fePath ../../apps/frontend --itPath ../../apps/backend-e2e --packageName @use-miller/shared-api-client"
    },
    "dependencies": {
        "@darraghor/nest-backend-libs": "2.18.25",
        "@nestjs/bull": "0.6.3",
        "@nestjs/cli": "9.2.0",
        "@nestjs/common": "9.3.9",
        "@nestjs/config": "2.3.1",
        "@nestjs/core": "9.3.9",
        "@nestjs/passport": "9.0.3",
        "@nestjs/platform-express": "9.3.9",
        "@nestjs/swagger": "6.2.1",
        "@nestjs/typeorm": "9.0.1",
        "auth0": "3.2.0",
        "bull": "4.10.4",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.0",
        "date-fns": "2.29.3",
        "dotenv": "16.0.3",
        "jwks-rsa": "3.0.1",
        "nodemailer": "6.9.1",
        "passport": "0.6.0",
        "passport-jwt": "4.0.1",
        "pg": "8.9.0",
        "reflect-metadata": "0.1.13",
        "res": "0.4.0",
        "resource": "0.8.1",
        "rimraf": "4.1.2",
        "rxjs": "7.8.0",
        "swagger-ui-express": "4.6.0",
        "tslog": "3.3.4",
        "typeorm": "0.3.12"
    },
    "devDependencies": {
        "@darraghor/eslint-plugin-nestjs-typed": "3.18.0",
        "@nestjs/schematics": "9.0.4",
        "@nestjs/testing": "9.3.9",
        "@types/auth0": "2.35.9",
        "@types/bull": "4.10.0",
        "@types/eslint": "8.21.1",
        "@types/express": "4.17.17",
        "@types/jest": "29.4.0",
        "@types/node": "18.13.0",
        "@types/nodemailer": "6.4.7",
        "@types/passport-jwt": "3.0.8",
        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "5.52.0",
        "@typescript-eslint/parser": "5.52.0",
        "eslint": "8.26.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jest": "27.2.1",
        "eslint-plugin-jsdoc": "40.0.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-sonarjs": "0.18.0",
        "eslint-plugin-unicorn": "45.0.2",
        "jest": "29.4.2",
        "jest-create-mock-instance": "2.0.0",
        "jest-junit": "15.0.0",
        "prettier": "2.8.4",
        "supertest": "6.3.3",
        "ts-jest": "29.0.5",
        "ts-loader": "9.4.2",
        "ts-node": "10.9.1",
        "tsconfig-paths": "4.1.2",
        "typescript": "4.9.5"
    }
}
